{
    "name": "obokaman/dateranger",
    "description": "Object Oriented calendar management",
    "keywords": [
        "date",
        "calendar",
        "events"
    ],
    "homepage": "https://github.com/obokaman/dateranger",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Albert Garcia",
            "email": "albert.garcia@obokaman.com",
            "homepage": "http://albert.garcia.gibert.es"
        }
    ],
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "DateRanger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DateRangerTest\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
